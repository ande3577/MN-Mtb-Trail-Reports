<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.dsanderson.morctrailreport"
    android:versionCode="109"
    android:versionName="0.1.9" >
    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    

    <uses-sdk android:minSdkVersion="7" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".morcTrailReportActivity"
            android:label="@string/app_name" >
            android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="PreferencesActivity"></activity>
        <activity android:name="RegionActivity"></activity>
        <activity android:name="ConditionFilterActivity"></activity>
        <activity android:name="AboutActivity"></activity>
        <activity android:name="AllReportActivity"></activity>
    </application>

</manifest>